{
  "name": "api-lead",
  "version": "1.0.0",
  "description": "Criacao de contas e contagem de visitantes",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:staged": "jest --passWithNoTests",
    "start": "node dist/src/main/server.js",
    "prestart": "rm -rf dist",
    "postinstall": "tsc -p tsconfig-build.json",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "pretest:integration": "env NODE_ENV=TEST npx sequelize-cli db:migrate && env NODE_ENV=TEST npx sequelize-cli db:seed:all",
    "postest:integration": "env NODE_ENV=TEST npx sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "Thiago Silva de Oliveira",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.3.8",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.0"
  },
